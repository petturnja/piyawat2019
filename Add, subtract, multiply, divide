namespace ConsoleAppsayhelloworld
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(calaulate());
            Console.WriteLine(calaulate1());
            Console.WriteLine(calaulate2());
            Console.WriteLine(calaulate3());
            sayHelloWorld();
            Console.ReadLine();
        }
        private static void sayHelloWorld()
        {
            for (int i = 0; i < 10; i++)
                Console.WriteLine("Hello World");
        }

        private static int calaulate()
        {
            int a = 10;
            int b = 5;
            int sum = a + b;
            return sum;
        }
        private static int calaulate1()
        {
            int a = 10;
            int b = 5;
            int sum1 = a - b;
            return sum1;
        }

        private static int calaulate2()
        {
            int a = 10;
            int b = 5;
            int sum2 = a * b;
            return sum2;
        }

        private static int calaulate3()
        {
            int a = 10;
            int b = 5;
            int sum3 = a / b;
            return sum3;
        }



    }
}
